            "NumberOfDaysOfRecallQAT": {
              "type": "string",
              "title": "t(13. Number of days of the recall/measurement per individual)",
              "enum": [
                "1 d",
                "2¨C7 d",
                ">7 d"
              ]
            },
            "SelectionOfRecallDaysQAT": {
              "type": "string",
              "title": "t(14. Selection of recall/measurement days)",
              "enum": [
