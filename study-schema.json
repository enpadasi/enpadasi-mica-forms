            },
            "MatchingData3QAT": {
              "type": "string",
              "title": "t(11.3 Food items matched to same food items in the table with similar moisture content (%, if 8 ¡Ù not quantified))"
            },
            "MatchingData4QAT": {
              "type": "string",
              "title": "t(11.4 Food items matched to a different food (%, if 8 ¡Ù not quantified))"
